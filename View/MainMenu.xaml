<Window x:Class="Inventarizacia.View.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inventarizacia.View"
        mc:Ignorable="d"
        Title="MainMenu" Height="768" Width="1366" WindowStartupLocation="CenterScreen" Icon="Image\Logo.png">
    <Grid Background="Black">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="25"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="1">
            <StackPanel Orientation="Horizontal">
                <TextBlock Foreground="White" FontSize="25">Аутентифицированный пользователь:</TextBlock>
                <TextBlock Foreground="White" FontSize="25" x:Name="q"></TextBlock>
            </StackPanel>
        </Grid>
        <Grid Grid.Row="0">
            <Frame Name="MainFrame"></Frame>
        </Grid>
    </Grid>
</Window>
